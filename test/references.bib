  booktitle = {2017 {{IEEE}} 28th {{Annual International Symposium}} on {{Personal}}, {{Indoor}}, and {{Mobile Radio Communications}} ({{PIMRC}})},
  author = {Boquet, Guillem and Vicario, Jose Lopez and Correa, Alejandro and Morell, Antoni and Rashdan, Ibrahim and D{\'i}az, Estefan{\'i}a Munoz and {de Ponte M{\"u}ller}, Fabian},
  year = {2017},
  month = oct,
  pages = {1--6},
  issn = {2166-9589},
  doi = {10.1109/PIMRC.2017.8292325},
  abstract = {In the new envisaged paradigm of vehicular communications, critical safety applications have strict requirements in terms of latency and robustness due to the critical nature of their mission that require periodic status exchange and asynchronous event notifications for a reliable cooperative awareness. Current technologies like IEEE 802.11p and recent solutions like fixed period beaconing or moderately reactive adaptive approaches do not cope with these requirements and lead to scalability problems in high density scenarios. With the aim of avoiding channel congestion in those scenarios we make use of trajectory prediction to reduce the number of transmissions required for a full awareness of the vehicle's position and speed. To do so, only a data prediction model of the vehicle's trajectory is sent over the channel. This allows to meet the latency requirements of safety applications while not relying exclusively on MAC congestion control protocols nor modifying the current standard MAC layer. In order to evaluate the proposed approach, we simulate a V2I network in a road intersection under high density traffic taking into account the packet delivery ratio and the predicted position error metrics. Finally, the simulation results show a better position awareness with a reduced channel load compared to the fixed periodic beaconing approach under high density traffic.},
  file = {/home/malte/Zotero/storage/N9K5ZDD8/Boquet et al. - 2017 - Trajectory prediction to avoid channel congestion .pdf;/home/malte/Zotero/storage/SEJ7FAZE/8292325.html},
  keywords = {channel congestion,cooperative communication,high density traffic,MAC congestion control protocols,MAC layer,Mathematical model,mobile communication,Predictive models,Reliability,Roads,Safety,Standards,telecommunication congestion control,telecommunication network reliability,telecommunication traffic,Trajectory,vehicular communications,wireless channels}
}


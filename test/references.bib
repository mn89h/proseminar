@inproceedings{chantaksinopasNetworkSelectionDelay2011,
  title = {Network Selection Delay Comparison of Network Selection Techniques for Safety Applications on {{VANET}}},
  booktitle = {2011 13th {{Asia}}-{{Pacific Network Operations}} and {{Management Symposium}}},
  author = {Chantaksinopas, Inthawadee and Oothongsap, Phoemphun and Prayote, Akara},
  year = {2011},
  month = sep,
  pages = {1--7},
  doi = {10.1109/APNOMS.2011.6077003},
